{% extends 'base.html.twig' %}

{% block title %}New DemandeRV
{% endblock %}

{% block body %}
	<h1>Create new DemandeRV</h1>

	<table class="table">
		<tr>
			<th></th>
			<th>Nom</th>
			<th>Prenom</th>
			<th>Contact</th>
			<th>Consulter</th>
		</tr>
		{% for medecin in medecins %}
			<tr>
				<td>{{medecin.getPhoto()}}</td>
				<td>{{medecin.getNom()}}</td>
				<td>{{medecin.getPrenom()}}</td>
				<td>{{medecin.getNumeroTelephone()}}</td>
				<td>
					<a href="{{path()}}"></td>
				</td>
			</tr>
		{% endfor %}
	</table>

	{{ include('demande_r_v/_form.html.twig') }}

	<a href="{{ path('demande_r_v_index') }}">back to list</a>
{% endblock %}
